<%- if @horario.errors.any? %>
  <%- @horario.errors.full_messages.each do |msg|%>
    alertify.error("<%=j msg %>");
  <%- end %>
<%- else %>
  $('#horarios').append("<%= j render('horarios/horario', horario: @horario) %>");
  $('.form-horario').trigger("reset");
  alertify.success("Horario Agregado");
<%- end %>
